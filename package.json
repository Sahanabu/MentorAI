{
  "name": "mentortrack-ai-web",
  "version": "1.0.0",
  "description": "Academic mentoring and performance tracking platform with AI predictions",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:ai\" \"npm run dev:web\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:ai": "cd ai-service && python src/main.py",
    "dev:web": "cd web-app && npm run dev",
    "build": "npm run build:backend && npm run build:web",
    "build:backend": "cd backend && npm run build",
    "build:web": "cd web-app && npm run build",
    "test": "npm run test:backend && npm run test:ai && npm run test:web",
    "test:backend": "cd backend && npm test",
    "test:ai": "cd ai-service && pytest",
    "test:web": "cd web-app && npm test",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "npm run setup:backend && npm run setup:ai && npm run setup:web",
    "setup:backend": "cd backend && npm install",
    "setup:ai": "cd ai-service && pip install -r requirements.txt",
    "setup:web": "cd web-app && npm install"
  },
  "keywords": [
    "education",
    "mentoring",
    "ai",
    "performance-tracking",
    "academic-management"
  ],
  "author": "MentorTrack AI Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "sonner": "^2.0.7",
    "tailwindcss-animate": "^1.0.7"
  }
}
